          <fileUrl>/home/sridhar/workspace/UIMAwebapp2/conf/models/en-pos-maxent.bin</fileUrl>
        </fileResourceSpecifier>
      </externalResource>
      <externalResource>
        <name>ChunkerModelSerFile</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>/home/sridhar/workspace/UIMAwebapp2/conf/models/en-chunker.bin</fileUrl>
        </fileResourceSpecifier>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>SentenceModel</key>
        <resourceName>SentenceModelSerFile</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TokenizerModel</key>
        <resourceName>TokenizerModelSerFile</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>POSModel</key>
        <resourceName>POSModelSerFile</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>ChunkerModel</key>
